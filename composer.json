{
  "name": "rama-can/laravel-imagekit",
  "description": "Package for interacting with ImageKit in Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "role": "Developer",
      "name": "Rama Can",
      "email": "heyramacan@gmai.com"
    }
